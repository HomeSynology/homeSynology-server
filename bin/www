#!/usr/bin/env node

const app = require('../app');
const port = normalizePort(process.env.PORT || '5001');
app.listen(port);

console.log('bin/www starting : 5001');
/**
 * Normalize a port into a number, string, or false.
 */

function normalizePort(val) {
  var port = parseInt(val, 10);

  if (isNaN(port)) {
    // named pipe
    return val;
  }

  if (port >= 0) {
    // port number
    return port;
  }

  return false;
}
